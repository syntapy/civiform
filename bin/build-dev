#! /bin/bash

set -e
set +x
export AWS_DEFAULT_REGION=us-west-2
REGION=us-west-2

aws ecr-public get-login-password --region us-east-1 | sudo docker login --username AWS --password-stdin public.ecr.aws/t1q6b4h2
sudo docker build -t civiform-dev --cache-from public.ecr.aws/t1q6b4h2/civiform-dev:latest .
sudo docker tag civiform-dev:latest public.ecr.aws/t1q6b4h2/civiform-dev:latest
sudo docker push public.ecr.aws/t1q6b4h2/civiform-dev:latest
