#!/bin/bash
sudo docker-compose -f infra/localstack.yaml up -d

echo "Waiting for localstack to set up. This may take a minute or two"
wget -O /tmp/localstack_health_check_slkhdfsg.html -o /tmp/localstack_health_check_slkhdfsg.log http://localhost:4566/health

##### Wait maybe 5s for services to change from 'starting' to 'running'
sleep 5

echo "Localstack should be ready now"
